local function YHQYQP_fake_script() -- Tab1.Tab1Script 
	local script = Instance.new('LocalScript', Tab1)

	local button = script.Parent
	local mainFrame = button.Parent
	
	local tab1Container = mainFrame:WaitForChild("Tab1Container")
	local tab2Container = mainFrame:WaitForChild("Tab2Container")
	local tab3Container = mainFrame:WaitForChild("Tab3Container")
	
	button.MouseButton1Click:Connect(function()
		tab1Container.Visible = true
		tab2Container.Visible = false
		tab3Container.Visible = false
	end)
	
end
coroutine.wrap(YHQYQP_fake_script)()
