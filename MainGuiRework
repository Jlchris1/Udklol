-- Scripts:

local function FIRQJZQ_fake_script() -- FlyGui.Script 
	local script = Instance.new('Script', FlyGui)

	-- Put this LocalScript inside your GUI button
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))() -- Replace this with your custom code
		-- Your code goes here:
	end)
end
coroutine.wrap(FIRQJZQ_fake_script)()
