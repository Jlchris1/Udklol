coroutine.wrap(HYMHFJD_fake_script)()
local function TEABWUK_fake_script() -- JLAdminGuiV1.RBGoutlineCode 
	local script = Instance.new('LocalScript', JLAdminGuiV1)

	local gui = script.Parent
	local borderSpeed = 2 -- Adjust this to make the color cycle faster/slower
	
	local hue = 0
	
	while true do
		hue = (hue + borderSpeed / 255) % 1
		local color = Color3.fromHSV(hue, 1, 1)
		gui.BorderColor3 = color
		wait()
	end
	
end
coroutine.wrap(TEABWUK_fake_script)()
local function POKYUC_fake_script() -- DestroyGui.Script 
	local script = Instance.new('Script', DestroyGui)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		local player = game.Players.LocalPlayer
		local gui = player:WaitForChild("PlayerGui"):FindFirstChild("JLadminGui")
	
		if gui then
			gui:Destroy()
		else
			warn("JLadminGui not found!")
		end
	end)
	
end
coroutine.wrap(POKYUC_fake_script)()
