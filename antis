local function TYZTXV_fake_script() -- anti0qqgui.wallz 
	local script = Instance.new('Script', anti0qqgui)

	function Click(mouse)
	
		-- Define the wall dimensions and properties
		local wallDistance = 230  -- Distance from the center to place the walls
		local wallThickness = 35  -- Thickness of the walls
		local wallHeight = 150  -- Height of the walls
		local wallColor = BrickColor.new("Grey") -- Color of the walls
		local wallMaterial = Enum.Material.Plastic  -- Material of the walls (you can change this)
		local wallTransparency = 0  -- Set to 0 for fully visible walls
	
		-- Wait for the Workspace to load (if necessary)
		local workspace = game:GetService("Workspace")
	
		-- Define the center of the area where the walls will be placed
		local centerPosition = Vector3.new(0, 0, 0)
	
		-- Function to create a wall with proper settings
		local function createWall(position, size)
			local wall = Instance.new("Part")
			wall.Size = size
			wall.Position = position
			wall.Anchored = true  -- Ensure the wall stays in place
			wall.CanCollide = true  -- Enable collisions for the wall
			wall.BrickColor = wallColor  -- Set the color of the walls
			wall.Material = wallMaterial  -- Set the material of the walls
			wall.Transparency = wallTransparency  -- Ensure the walls are visible
			wall.CastShadow = false  -- Disable shadow casting for the wall
			wall.Parent = workspace
			print("Wall created at position: ", position, " Size: ", size)
		end
	
		-- Calculate the total size based on the distance
		local totalWidth = wallDistance * 2  -- Total width of the area (distance to both sides)
		local totalHeight = wallHeight
	
		-- Wall 1: Front
		createWall(Vector3.new(centerPosition.X, centerPosition.Y, centerPosition.Z + wallDistance),
			Vector3.new(totalWidth, totalHeight, wallThickness))
	
		-- Wall 2: Back
		createWall(Vector3.new(centerPosition.X, centerPosition.Y, centerPosition.Z - wallDistance),
			Vector3.new(totalWidth, totalHeight, wallThickness))
	
		-- Wall 3: Left
		createWall(Vector3.new(centerPosition.X - wallDistance, centerPosition.Y, centerPosition.Z),
			Vector3.new(wallThickness, totalHeight, totalWidth))
	
		-- Wall 4: Right
		createWall(Vector3.new(centerPosition.X + wallDistance, centerPosition.Y, centerPosition.Z),
			Vector3.new(wallThickness, totalHeight, totalWidth))
	
		-- Wall 5: Top (roof)
		createWall(Vector3.new(centerPosition.X, centerPosition.Y + totalHeight / 2, centerPosition.Z),
			Vector3.new(totalWidth + wallThickness, wallThickness, totalWidth + wallThickness))
	
		-- Wall 6: Bottom (floor)
		createWall(Vector3.new(centerPosition.X, centerPosition.Y - totalHeight / 2, centerPosition.Z),
			Vector3.new(totalWidth + wallThickness, wallThickness, totalWidth + wallThickness))
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(TYZTXV_fake_script)()
local function YZXH_fake_script() -- anti0qqgui.godmode 
	local script = Instance.new('Script', anti0qqgui)

	function Click(mouse)
	
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
	
		-- Function to initialize and set up behavior on character spawn/respawn
		local function initializeCharacter()
			-- Ensure the character exists and components are fully loaded before proceeding
			if not player.Character or not player.Character:FindFirstChild("Humanoid") then
				player.Character:WaitForChild("Humanoid")
			end
	
			-- Use pcall to safely remove specific objects if they exist
			pcall(function()
				for _, v in next, player.Character:GetChildren() do
					if v.Name == 'VelocityDamage' or v.Name == 'GetPicked' or v.Name == 'ArmAngleUpdate' then
						v:Destroy()
					end
				end
			end)
	
			-- Handle Humanoid cloning and health management
			pcall(function()
				local humanoid = player.Character:FindFirstChild("Humanoid")
				if humanoid then
					local clone = humanoid:Clone()
					clone.Parent = player.Character
					humanoid.Parent = nil
	
					-- Monitor the cloned humanoid's health
					clone:GetPropertyChangedSignal('Health'):Connect(function()
						if clone.Health <= 0 then
							humanoid.Parent = player.Character
							wait(1)
							humanoid:Destroy()
						end
					end)
	
					-- Set the camera to follow the player's character
					workspace.CurrentCamera.CameraSubject = player.Character
				end
			end)
		end
	
		-- Connect to CharacterAdded to reinitialize upon respawn
		player.CharacterAdded:Connect(function()
			-- Add a small delay to ensure the character is fully loaded
			wait(0.1)
			initializeCharacter()
		end)
	
		-- Call the initialize function when the script is first run, if the character already exists
		if player.Character then
			-- Add a small delay in case the character isn't fully loaded yet
			wait()
			initializeCharacter()
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(YZXH_fake_script)()
local function CJIRLR_fake_script() -- anti0qqgui.remove touch interest 
	local script = Instance.new('Script', anti0qqgui)

	function Click(mouse)
	
		-- Function to delete all TouchInterest instances inside anvils
		local function deleteTouchInterestsInAnvils()
			for _, anvil in pairs(workspace:GetDescendants()) do
				if anvil:IsA("Model") and anvil.Name == "Anvil" then
					-- Loop through all descendants of the anvil model
					for _, part in pairs(anvil:GetDescendants()) do
						-- If a descendant is a TouchInterest, remove it
						if part:IsA("TouchTransmitter") then
							part:Destroy()
						end
					end
				end
			end
		end
	
		-- Run the function in a loop to constantly delete TouchInterest instances
		while true do
			deleteTouchInterestsInAnvils()
			task.wait(0.35) -- Wait 1 second before running the loop again
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(CJIRLR_fake_script)()
local function VVUWFM_fake_script() -- anti0qqgui.remove collisions 
	local script = Instance.new('Script', anti0qqgui)

	function Click(mouse)
	
		-- Function to disable all collision-related properties for the player's body parts
		local function disableAllBodyCollisions(character)
			for _, part in pairs(character:GetDescendants()) do
				if part:IsA("BasePart") then
					-- Disable all collision-related properties
					part.CanCollide = false
					part.CanTouch = false
					part.CanQuery = false
				end
			end
		end
	
		-- Connect to player respawn
		local player = game:GetService("Players").LocalPlayer
	
		player.CharacterAdded:Connect(function(character)
			-- Wait for the character to fully load
			character:WaitForChild("HumanoidRootPart")
	
			-- Continuous loop to disable collisions every 0.5 seconds
			while true do
				disableAllBodyCollisions(character)
				wait(0.1) -- Adjust delay as needed
			end
		end)
	
		-- Initial run in case the script starts after the character is already loaded
		if player.Character then
			-- Continuous loop to disable collisions every 0.5 seconds
			while true do
				disableAllBodyCollisions(player.Character)
				wait(0.5) -- Adjust delay as needed
			end
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(VVUWFM_fake_script)()
local function WCLRWNC_fake_script() -- anti0qqgui.pick destroy anvils 
	local script = Instance.new('Script', anti0qqgui)

	function Click(mouse)
	
		while true do
			-- Loop through all children in the workspace
			for _, item in pairs(workspace:GetChildren()) do
				-- Check if the item is a MeshPart and named "Anvil"
				if item:IsA("MeshPart") and item.Name == "Anvil" then
					local args = {
						[1] = item,  -- Use the current anvil item
						[2] = Vector3.new(0, -0/0, 0)  -- Use a very large value to simulate void movement
					}
	
					-- Fire the Picking remote with the current anvil
					game:GetService("Players").LocalPlayer.Character.Picking:FireServer(unpack(args))
	
					-- Wait a brief moment before putting it down to simulate fast movement
					task.wait(0.1)  -- A very short wait time to give the server a moment to process the pick
	
					-- Fire the PuttingDown remote to simulate fast movement to the void
					game:GetService("Players").LocalPlayer.Character.PuttingDown:FireServer()
				end
			end
	
			-- Wait a brief moment before checking again (optional, to avoid flooding)
			task.wait() -- Shorter wait time for quicker iterations
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(WCLRWNC_fake_script)()
local function THKP_fake_script() -- anti0qqgui.remove velocity of anvils 
	local script = Instance.new('Script', anti0qqgui)

	function Click(mouse)
	
		-- Function to set the velocity of all MeshPart anvils to zero
		local function setVelocityToZero()
			for _, item in pairs(workspace:GetChildren()) do
				-- Check if the item is a MeshPart and named "Anvil"
				if item:IsA("MeshPart") and item.Name == "Anvil" then
					item.Velocity = Vector3.new(0, 0, 0) -- Set velocity to zero
					item.RotVelocity = Vector3.new(0, 0, 0) -- Optionally, set rotational velocity to zero
				end
			end
		end
	
		-- Initial run to set velocities to zero
		setVelocityToZero()
	
		-- Continuous loop to monitor and reset the velocities
		while true do
			setVelocityToZero()  -- Set the velocity of anvils to zero
			task.wait(0.35) -- Adjust the wait time as needed (0.1 seconds in this case)
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(THKP_fake_script)()
local function IQST_fake_script() -- anti0qqgui.fly gui 
