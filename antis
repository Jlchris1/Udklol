local function RBETGUL_fake_script() -- antipick.aap 
	local script = Instance.new('Script', antipick)

	function Click(mouse)
	
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
	
		-- Function to refresh all accessories (including hair) on the player's character
		local function refreshCharacterItems()
			local character = player.Character or player.CharacterAdded:Wait()
	
			-- Ensure all parts and accessories are loaded before proceeding
			character:WaitForChild("Humanoid")
	
			-- Refresh all accessories (hair, hats, gear)
			for _, accessory in pairs(character:GetChildren()) do
				if accessory:IsA("Accessory") and accessory:FindFirstChild("Handle") then
					local clone = accessory:Clone()  -- Clone the accessory
					clone.Parent = character         -- Attach the clone to the character
					wait()                        -- Small delay to ensure the clone is attached
					accessory:Destroy()              -- Destroy the old accessory after cloning
				end
			end
		end
	
		-- Function to handle character respawn and initialize the refresh process upon respawn
		local function onCharacterAdded()
			wait()  -- Brief delay to ensure character is fully loaded
			refreshCharacterItems()  -- Refresh items after respawn
		end
	
		-- Connect to CharacterAdded event to run the script upon respawn
		player.CharacterAdded:Connect(onCharacterAdded)
	
		-- Call the function when the script is first run, if the character already exists
		if player.Character then
			onCharacterAdded()  -- Refresh immediately if the character is already loaded
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(RBETGUL_fake_script)()
local function VZJAUAK_fake_script() -- antipick.ap 
	local script = Instance.new('Script', antipick)

	function Click(mouse)
	
		local Players = game:GetService("Players")
		local LocalPlayer = Players.LocalPlayer
	
	
		local function deleteStabler(character)
			if character then
	
				for _, child in pairs(character:GetChildren()) do
					if child.Name == "Stabler" then
						child:Destroy()  
						print("Deleted Stabler from:", character.Parent.Name)
					end
				end
			end
		end
	
	
		local function monitorPlayersStabler()
			while true do
				for _, player in pairs(Players:GetPlayers()) do
					if player ~= LocalPlayer then  
						local character = player.Character
						if character then
							deleteStabler(character)  
						end
					end
				end
	
	
				wait()  
			end
		end
	
	
		monitorPlayersStabler()
	
	
		Players.PlayerAdded:Connect(function(player)
			player.CharacterAdded:Connect(function(character)
				if player ~= LocalPlayer then  
					deleteStabler(character)  
				end
			end)
		end)
	
	
		for _, player in pairs(Players:GetPlayers()) do
			if player ~= LocalPlayer then  
				player.CharacterAdded:Connect(function(character)
					deleteStabler(character)  
				end)
	
	
				if player.Character then
					deleteStabler(player.Character) 
				end
			end
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(VZJAUAK_fake_script)()
local function GCWGH_fake_script() -- antipick.mb 
	local script = Instance.new('Script', antipick)

	function Click(mouse)
	
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
	
		-- Function to make all body parts massless
		local function makeBodyPartsMassless()
			local character = player.Character or player.CharacterAdded:Wait()
	
			-- Iterate through all parts of the character
			for _, part in pairs(character:GetChildren()) do
				if part:IsA("BasePart") then  -- Check if it's a body part (BasePart)
					part.Massless = true  -- Set the part to be massless
				end
			end
		end
	
		-- Function to handle respawn and initialize massless body
		local function onCharacterAdded()
			wait()  -- Brief delay to ensure the character has loaded
			makeBodyPartsMassless()  -- Make body parts massless after spawn
		end
	
		-- Connect to CharacterAdded event to run the script upon respawn
		player.CharacterAdded:Connect(onCharacterAdded)
	
		-- Call the function initially if the character already exists when the script is run
		if player.Character then
			onCharacterAdded()
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(GCWGH_fake_script)()
local function FQTXXO_fake_script() -- antipick.anchor upon respawn 
	local script = Instance.new('Script', antipick)

	function Click(mouse)
	
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
	
		-- Function to anchor all body parts for 0.8 seconds
		local function anchorBodyForTime()
			local character = player.Character or player.CharacterAdded:Wait()
	
			-- Ensure the character's Humanoid is fully loaded
			character:WaitForChild("Humanoid")
	
			-- Anchor all the body parts
			for _, part in pairs(character:GetChildren()) do
				if part:IsA("BasePart") then
					part.Anchored = true -- Anchor the body part
				end
			end
	
			-- Wait for 0.8 seconds
			wait(1)
	
			-- Un-anchor the body parts
			for _, part in pairs(character:GetChildren()) do
				if part:IsA("BasePart") then
					part.Anchored = false -- Un-anchor the body part
				end
			end
		end
	
		-- Function to handle character respawn
		local function onCharacterAdded()
			wait() -- Small delay to ensure character is fully loaded
			anchorBodyForTime() -- Anchor the body for 0.8 seconds after respawn
		end
	
		-- Connect the CharacterAdded event to run the function upon respawn
		player.CharacterAdded:Connect(onCharacterAdded)
	
		-- Initialize when the script is first run, if the character already exists
		if player.Character then
			onCharacterAdded() -- Anchor the body if the character is already loaded
		end
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(FQTXXO_fake_script)()
