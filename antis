local function LEHEX_fake_script() -- antipick.anti bodypart pick 
	local script = Instance.new('Script', antipick)

	function Click(mouse)
	
		local Players = game:GetService("Players")
		local LocalPlayer = Players.LocalPlayer
	
		
		local function deleteStabler(character)
			if character then
				
				for _, child in pairs(character:GetChildren()) do
					if child.Name == "Stabler" then
						child:Destroy()  
						print("Deleted Stabler from:", character.Parent.Name)
					end
				end
			end
		end
	
		
		local function monitorPlayersStabler()
			while true do
				for _, player in pairs(Players:GetPlayers()) do
					if player ~= LocalPlayer then  
						local character = player.Character
						if character then
							deleteStabler(character)  
						end
					end
				end
	
				
				wait()  
			end
		end
	
		
		monitorPlayersStabler()
	
		
		Players.PlayerAdded:Connect(function(player)
			player.CharacterAdded:Connect(function(character)
				if player ~= LocalPlayer then  
					deleteStabler(character)  
				end
			end)
		end)
	
		
		for _, player in pairs(Players:GetPlayers()) do
			if player ~= LocalPlayer then  
				player.CharacterAdded:Connect(function(character)
					deleteStabler(character)  
				end)
	
				
				if player.Character then
					deleteStabler(player.Character) 
				end
			end
		end
	
	
	
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(LEHEX_fake_script)()
local function BRIFNU_fake_script() -- antipick.anti accs pick 
	local script = Instance.new('Script', antipick)

	function Click(mouse)
	
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
	
		-- Function to refresh the player's hair and worn gear by duplicating first, then destroying old ones
		local function refreshPlayerGearAndHair()
			local character = player.Character or player.CharacterAdded:Wait()
	
			-- Duplicate hair accessories (hat-type accessories) first, then destroy old ones
			for _, accessory in pairs(character:GetChildren()) do
				if accessory:IsA("Accessory") and accessory:FindFirstChild("Handle") and accessory.AccessoryType == Enum.AccessoryType.Hat then
					local clone = accessory:Clone()
					clone.Parent = character
					wait(0.1) -- Small delay to ensure the clone is set
					accessory:Destroy() -- Destroy the old hair accessory
				end
			end
	
			-- Duplicate worn gear (other non-hair accessories) first, then destroy old ones
			for _, accessory in pairs(character:GetChildren()) do
				if accessory:IsA("Accessory") and accessory:FindFirstChild("Handle") and 
					(accessory.AccessoryType == Enum.AccessoryType.Back or 
						accessory.AccessoryType == Enum.AccessoryType.Neck or 
						accessory.AccessoryType == Enum.AccessoryType.Shoulder or
						accessory.AccessoryType == Enum.AccessoryType.Waist or 
						accessory.AccessoryType == Enum.AccessoryType.Front or
						accessory.AccessoryType == Enum.AccessoryType.Hip) then
	
					local clone = accessory:Clone()
					clone.Parent = character
					wait(0.01) -- Small delay to ensure the clone is set
					accessory:Destroy() -- Destroy the old worn gear
				end
			end
		end
	
		-- Function to handle character respawn
		local function onCharacterAdded()
			-- Wait a brief moment for the character to fully load
			wait()
			refreshPlayerGearAndHair() -- Refresh hair and worn gear
		end
	
		-- Connect the function to the CharacterAdded event
		player.CharacterAdded:Connect(onCharacterAdded)
	
		-- Call the function when the script is first run, if the character already exists
		if player.Character then
			onCharacterAdded() -- Refresh immediately if the character is already loaded
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(BRIFNU_fake_script)()
local function KWIYODJ_fake_script() -- antipick.loop massless body 
	local script = Instance.new('Script', antipick)

	function Click(mouse)
	
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
	
		-- Function to make all body parts massless
		local function makeBodyPartsMassless()
			local character = player.Character or player.CharacterAdded:Wait()
	
			-- Iterate through all parts of the character
			for _, part in pairs(character:GetChildren()) do
				if part:IsA("BasePart") then  -- Check if it's a body part (BasePart)
					part.Massless = true  -- Set the part to be massless
				end
			end
		end
	
		-- Function to handle respawn and initialize massless body
		local function onCharacterAdded()
			wait()  -- Brief delay to ensure the character has loaded
			makeBodyPartsMassless()  -- Make body parts massless after spawn
		end
	
		-- Connect to CharacterAdded event to run the script upon respawn
		player.CharacterAdded:Connect(onCharacterAdded)
	
		-- Call the function initially if the character already exists when the script is run
		if player.Character then
			onCharacterAdded()
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(KWIYODJ_fake_script)()
