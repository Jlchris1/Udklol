end
coroutine.wrap(ULWL_fake_script)()
local function KWIBL_fake_script() -- sas.Script 
	local script = Instance.new('Script', sas)

	function Click(mouse)
	
		local syn = getgenv().syn
		getgenv().syn = false
	
		local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()local Window = OrionLib:MakeWindow({Name = "JLhub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})local Tab = Window:MakeTab({
			Name = "regens etc",
			Icon = "rbxassetid://4483345998",
			PremiumOnly = false
		}) Tab:AddButton({
			Name = "regen energy core",
			Callback = function()
				fireclickdetector(workspace.EnergyCore.Regen.ClickDetector)
			end    
		}) Tab:AddButton({
			Name = "start energy core explosion",
			Callback = function()
				fireclickdetector(workspace.EnergyCore.Begin.ClickDetector)
			end    
		}) Tab:AddButton({
			Name = "fill energy core",
			Callback = function()
				fireclickdetector(workspace.EnergyCore.Fill.ClickDetector)
			end    
		}) Tab:AddButton({
			Name = "Regen whole map",
			Callback = function()
				fireclickdetector(workspace.city.Regen.ClickDetector)
			end    
		}) Tab:AddButton({
			Name = "stop auto regen map",
			Callback = function()
				_G.toggle = false
			end    
		}) local Tab = Window:MakeTab({
			Name = "dupe",
			Icon = "rbxassetid://4483345998",
			PremiumOnly = false
		}) Tab:AddButton({
			Name = "dupe pump shotgun",
			Callback = function()
				-- clone tool script
				local copy = game:GetService("Players").LocalPlayer.Backpack["Pump Shotgun"]:Clone()
	
				copy.Parent = game.Players.LocalPlayer.Backpack
			end    
		}) Tab:AddButton({
			Name = "dupe M4A1",
			Callback = function()
				-- clone tool script
				local copy = game:GetService("Players").LocalPlayer.Backpack.M4A1:Clone()
	
				copy.Parent = game.Players.LocalPlayer.Backpack
			end    
		}) local Tab = Window:MakeTab({
			Name = "alarms",
			Icon = "rbxassetid://4483345998",
			PremiumOnly = false
		}) Tab:AddButton({
			Name = "alarm 1",
			Callback = function()
				fireclickdetector(workspace.city["SCP Alarm controll"]:GetChildren()[3].Buttons.play.ClickDetector)
			end    
		}) Tab:AddButton({
			Name = "alarm 2",
			Callback = function()
				fireclickdetector(workspace.city["SCP Alarm controll"]:GetChildren()[7].Buttons.play.ClickDetector)
			end    
		}) Tab:AddButton({
			Name = "alarm 3",
			Callback = function()
				fireclickdetector(workspace.city["SCP Alarm controll"]["Containment Breach alarm"].Buttons.play.ClickDetector)
			end    
		}) Tab:AddButton({
			Name = "alarm 4",
			Callback = function()
				fireclickdetector(workspace.city["SCP Ray To Shoot SCP-106 [GATE-A] Alarm"].siren.play.ClickDetector)
			end    
		})
	
		getgenv().syn = syn
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(KWIBL_fake_script)()
