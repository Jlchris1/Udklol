-- Scripts:

local function WCZWG_fake_script() -- bps.Script 
	local script = Instance.new('Script', bps)

	function Click(mouse)
	
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Jlchris1/Udklol/refs/heads/main/maingui"))()
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(WCZWG_fake_script)()
