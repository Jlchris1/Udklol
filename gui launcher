local function SLGC_fake_script() -- TAUgui.Script 
	local script = Instance.new('Script', TAUgui)

	function Click(mouse)
	
		local syn = getgenv().syn
		getgenv().syn = false
	
		local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()local Window = OrionLib:MakeWindow({Name = "", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})local Tab = Window:MakeTab({
			Name = "pro shit",
			Icon = "rbxassetid://4483345998",
			PremiumOnly = false
		}) Tab:AddButton({
			Name = "anti break bones",
			Callback = function()
				game:GetService("Players").LocalPlayer.BonesEnabled:destroy()
			end    
		}) Tab:AddButton({
			Name = "GOD MODE REWORKED TRUE INF HP",
			Callback = function()
				_G.toggle = true
	
				while _G.toggle do
					wait()
					local args = {
						[1] = {
							["A"] = 0/0,
							["B"] = false,
							["C"] = game:GetService("Players").LocalPlayer.Character:FindFirstChild("Space Sandwich"),
							["reason"] = "UseItem"
						}
					}
	
					game:GetService("ReplicatedStorage").Assets.Signals.Event:FireServer(unpack(args))
	
				end
			end    
		}) Tab:AddButton({
			Name = "turn off auto god mode and any other loops",
			Callback = function()
				_G.toggle = false
			end    
		}) Tab:AddButton({
			Name = "tp to bunker",
			Callback = function()
				local teleport_table = {
					location1 = Vector3.new(75489.671875, 31957.552734375, -141.92681884765625), -- your desired position
					location2 = Vector3.new()  -- your desired position
				}
	
				local tween_s = game:GetService('TweenService')
				local tweeninfo = TweenInfo.new(0,Enum.EasingStyle.Linear)
	
				local lp = game.Players.LocalPlayer
	
				function bypass_teleport(v)
					if lp.Character and 
						lp.Character:FindFirstChild('HumanoidRootPart') then
						local cf = CFrame.new(v)
						local a = tween_s:Create(lp.Character.HumanoidRootPart,tweeninfo,{CFrame=cf})
						a:Play()
						-- a.Completed:Wait()
						-- print('Teleporting Done!')
					end
				end
	
				bypass_teleport(teleport_table.location1)
			end    
		}) Tab:AddButton({
			Name = "loop tp into bunker",
			Callback = function()
				_G.toggle = true
	
				while _G.toggle do
					wait(1)
					local teleport_table = {
						location1 = Vector3.new(75489.671875, 31957.552734375, -141.92681884765625), -- your desired position
						location2 = Vector3.new()  -- your desired position
					}
	
					local tween_s = game:GetService('TweenService')
					local tweeninfo = TweenInfo.new(0,Enum.EasingStyle.Linear)
	
					local lp = game.Players.LocalPlayer
	
					function bypass_teleport(v)
						if lp.Character and 
							lp.Character:FindFirstChild('HumanoidRootPart') then
							local cf = CFrame.new(v)
							local a = tween_s:Create(lp.Character.HumanoidRootPart,tweeninfo,{CFrame=cf})
							a:Play()
							-- a.Completed:Wait()
							-- print('Teleporting Done!')
						end
					end
	
					bypass_teleport(teleport_table.location1)
				end
			end    
		})
	
		getgenv().syn = syn
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
