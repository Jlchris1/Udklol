-- Scripts:

local function BVRO_fake_script() -- unanchoredgui.Script 
	local script = Instance.new('Script', unanchoredgui)

	function Click(mouse)
	
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Jlchris1/Udklol/refs/heads/main/opunanchoredgui"))()
	
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(BVRO_fake_script)()
