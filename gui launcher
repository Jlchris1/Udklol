local function LKXCE_fake_script() -- fireallremotes.Script 
	local script = Instance.new('Script', fireallremotes)

	function Click(mouse)
	
		local ReplicatedStorage = game:GetService("ReplicatedStorage")
	
		-- Function to recursively search and handle RemoteEvents and RemoteFunctions
		local function findAndFireRemotesInContainer(container)
			for _, obj in pairs(container:GetDescendants()) do
				-- Detect if the object is a RemoteEvent or RemoteFunction
				if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
					-- Get the full path of the remote to distinguish it even if it shares the same name
					local fullPath = obj:GetFullName()
	
					-- Log the RemoteEvent or RemoteFunction
					print("Found remote: " .. obj.Name .. " at path: " .. fullPath)
	
					-- If it's a RemoteEvent, we fire it
					if obj:IsA("RemoteEvent") then
						obj:FireServer() -- Fire the RemoteEvent
						print("Fired RemoteEvent: " .. obj.Name)
					elseif obj:IsA("RemoteFunction") then
						-- Fire the RemoteFunction by invoking it with default arguments (you can customize this)
						local success, result = pcall(function()
							return obj:InvokeServer() -- Optionally pass arguments here
						end)
	
						if success then
							print("Successfully invoked RemoteFunction: " .. obj.Name .. " | Result: " .. tostring(result))
						else
							print("Failed to invoke RemoteFunction: " .. obj.Name .. " | Error: " .. result)
						end
					end
				elseif obj:IsA("Folder") or obj:IsA("Model") then
					-- If the object is a folder or a container-like object, recursively check its descendants
					print("Searching inside folder/model: " .. obj.Name)
					findAndFireRemotesInContainer(obj)
				end
			end
		end
	
		-- Function to start searching for all remotes in ReplicatedStorage
		local function fireAllRemotes()
			-- Search through ReplicatedStorage and fire remotes
			print("Starting remote search in ReplicatedStorage...")
			findAndFireRemotesInContainer(ReplicatedStorage)
		end
	
		-- Call the function to fire all remotes
		fireAllRemotes()
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(LKXCE_fake_script)()
local function WUYT_fake_script() -- refreshscript.Script 
	local script = Instance.new('Script', refreshscript)

	function Click(mouse)
	
		-- Variables
		local Players = game:GetService("Players")
	
		-- Function to handle player chatting
		local function onChatted(player, message)
			local character = player.Character
	
			if message:lower() == ".re" then
				if character and character:FindFirstChild("Humanoid") then
					-- Save the location where the player is resetting
					local respawnLocation = character.HumanoidRootPart.Position
	
					-- Set health to 0 to trigger respawn
					character.Humanoid.Health = 0
	
					-- Wait for the player to respawn
					local newCharacter = player.CharacterAdded:Wait()
	
					-- Teleport to the saved respawn location immediately
					local humanoidRootPart = newCharacter:WaitForChild("HumanoidRootPart")
					humanoidRootPart.CFrame = CFrame.new(respawnLocation)
	
					-- Restore health to max
					local humanoid = newCharacter:WaitForChild("Humanoid")
					humanoid.Health = humanoid.MaxHealth
				end
			end
		end
	
		-- Function to connect the chat event to the player's new character
		local function setupChatEvent(player)
			player.Chatted:Connect(function(message)
				onChatted(player, message)
			end)
		end
	
		-- Function to connect the PlayerAdded event for new players
		local function onPlayerAdded(player)
			-- Setup chat event for the new player
			setupChatEvent(player)
	
			-- Also connect to their CharacterAdded event to setup for respawns
			player.CharacterAdded:Connect(function(character)
				-- Re-setup the chat event for the new character
				setupChatEvent(player)
			end)
	
			-- Check if the player already has a character and setup the chat event if they do
			if player.Character then
				setupChatEvent(player)
			end
		end
	
		-- Connect to existing players
		for _, player in pairs(Players:GetPlayers()) do
			onPlayerAdded(player)
		end
	
		-- Connect the PlayerAdded event for new players
		Players.PlayerAdded:Connect(onPlayerAdded)
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(WUYT_fake_script)()
